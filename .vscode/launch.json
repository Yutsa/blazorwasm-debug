{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch and Debug Standalone Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "browser": "edge",
            "url": "http://localhost:5186",
            "cwd": "${workspaceFolder}/Server"
        },
        {
            "name": "Debug Server",
            "type": "coreclr",
            "request": "attach",
            "processName": "blazorwasm.Server"
        },
    ]
}